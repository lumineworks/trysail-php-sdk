{
    "name": "trysail/sdk",
    "description": "Official PHP SDK for Trysail WireGuard Control Plane",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "trysail",
        "wireguard",
        "vpn",
        "control-plane",
        "sdk"
    ],
    "authors": [
        {
            "name": "Trysail Team"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Trysail\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trysail\\SDK\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trysail\\SDK\\Laravel\\TrysailServiceProvider"
            ],
            "aliases": {
                "Trysail": "Trysail\\SDK\\Laravel\\Facades\\Trysail"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src --level=max",
        "cs": "phpcs --standard=PSR12 src",
        "cs-fix": "phpcbf --standard=PSR12 src"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}